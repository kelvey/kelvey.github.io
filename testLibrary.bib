@inproceedings{gifford_role_2018,
	title = {The role of founder knowledge in the survival and growth of knowledge intensive new ventures},
	url = {https://eaepe.org},
	abstract = {This paper examines the impacts of three types of founder and founder team knowledge on the survival and growth of knowledge-intensive new ventures: entrepreneurial, academic and same-industry knowledge. We utilize unique survey data of young, small European firms across sectors, which have been matched with data on subsequent firm performance. In terms of likelihood of survival, our results indicate the importance of previous entrepreneurial knowledge and same-industry knowledge. In terms of growth, our results using quantile regression indicate that both entrepreneurial knowledge and academic knowledge from previous work experience are positively associated with high growth rates, whereas results for same-industry knowledge are mixed.},
